{
  "compilerOptions": {
    "outDir": "out",
    "module": "commonjs",
    "target": "es2015",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noEmitOnError": true,
    "downlevelIteration": true,
    "sourceMap": true,
    "rootDir": "."
  },
  "exclude": [
    "node_modules",
    ".vscode-test"
  ]
}
